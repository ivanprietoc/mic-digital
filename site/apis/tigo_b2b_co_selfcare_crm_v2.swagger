definitions:
  servicesParameters:
    properties:
      serviceCollection:
        type: array
        items: 
           $ref: '#/definitions/service'
  service: 
    type: object
    properties: 
      partnum:
        type: string
  updateServicesParameters:
    properties:
      service:
        type: array
        items: 
           $ref: '#/definitions/serviceList'
  serviceList: 
    type: object
    properties:
      id:
        type: integer 
      partnum:
        type: string  
  postAccountsParameters: 
    type: object
    properties: 
      consultantId:
        type: integer 
        
  servicesList:
    type: object
    properties: 
      payload: 
        type: string
      msgCode: 
        type: string
      description: 
        type: string
      severity: 
        type: string
  getServicesList:
    type: object
    properties: 
      payload:
        type: string
      service:
        type: object
        properties:
          id: 
            type: integer
          type: 
            type: string
          subType: 
            type: string
          description:
            type: string
          price: 
            type: integer
          taxPrice: 
            type: integer
          partNum: 
            type: string
          poidType:
            type: string
  details: 
    type: object
    properties: 
      msisdn:
        type: integer
      company: 
        type: string
      callType: 
        type: string
      callFrom: 
        type: string
      callDateTime: 
        type: string
        format: date-time
      callDuration: 
        type: integer
      airTime: 
        type: integer
      airAmount: 
        type: string 
      interconnectionTime: 
        type: integer
      interconnectionAmount: 
        type: string 
      callAmount: 
        type: string    
  portfolioList:
    type: array
    items:
      type: object
      properties:
        idSede:
          type: string
        nameSede:
          type: integer
        extensionsList:
          type: array
          items: 
            $ref: '#/definitions/extensions'
  extensions:
    type: object
    properties:
      number:
        type: integer
      recording: 
        type: string
      name: 
        type: string
  recordingsParameters:
    type: object
    properties: 
      extensions:
        type: array
