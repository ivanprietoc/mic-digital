parameters:
  partnum: 
    name: partnum
    in: path
    required: true
    type: integer
    description: "Customer Document"
  postServicesParameters: 
    name: postServicesParameters
    in: body
    required: true
    schema: 
      $ref: '#/definitions/servicesParameters'
    description: "Parameters needed to add a perfil"
  deleteServicesParameters: 
    name: deleteServicesParameters
    in: body
    required: true
    schema: 
      $ref: '#/definitions/servicesParameters'
    description: "Parameters needed to remove a perfil"
  updateServicesParameters: 
    name: updateServicesParameters
    in: body
    required: true
    schema: 
      $ref: '#/definitions/updateServicesParameters'
    description: "Parameters needed to update a perfil"
  postAccountsParameters: 
    name: postAccountsParameters
    in: body
    required: true
    schema: 
      $ref: '#/definitions/postAccountsParameters'
    description: "Parameters needed to process secondary transactions necessary to complete the purchase"
  nit:
    name: nit
    in: path
    required: true
    type: integer
    description: "It's the customer's nit."
  idCall:
    name: idCall
    in: path
    required: true
    type: integer
    description: "It's the call's id."
  idFormat:
    name: idFormat
    in: path
    required: true
    type: integer
    description: "It's the call's format."
  id:
    name: id
    in: query
    required: true
    type: integer
    description: "Identification Number"
  idType:
    name: idType
    in: query
    required: true
    type: string
    description: "Identifier Type of the Identification Number"
  accountId:
    name: accountId
    in: path
    required: true
    type: integer
    description: "Account Identification Number"
  profileId:
    name: profileId
    in: path
    required: true
    type: integer
    description: "Profile Identification Number"
  msisdn:
    name: msisdn
    in: query
    required: true
    type: integer
    description: "Customers MSISDN"
  MSISDN:
    name: MSISDN
    in: path
    required: true
    type: integer
    description: "Customers MSISDN"
  dateFrom:
    name: dateFrom
    in: query
    description: Date of start
    required: true
    type: string
  dateTill:
    name: dateTill
    in: query
    description: Date of end
    required: true
    type: integer
  documentId:
    name: documentId
    in: path
    required: true
    type: integer
    description: Is the document number.
  movil:
    name: movil
    in: query
    required: true
    type: integer
    description: Msisdn to consult.
  segment:
    name: segment
    in: query
    required: false
    type: string
    description: Segment to consult can be one of the CORPORATE / MASIVE values.
  document:
    name: document
    in: query
    required: false
    type: string
    description: Corresponds to the id of the document found in computer.
  additional:
    name: additional
    in: query
    required: false
    type: string
    description: Additional parameters.
  recordingsParameters: 
    name: recordingsParameters
    in: body
    required: true
    schema: 
      $ref: '#/definitions/recordingsParameters'
    description: "Parameters required to obtain the generated recordings for the client extensions for a defined period of time"
  authorization:
    name: Authorization
    in: header
    required: true
    type: string
    description: "Access Token provided by the Authorization Url"

